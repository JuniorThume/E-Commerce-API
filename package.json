    "express-async-error": "^0.0.2",
    "pg": "^8.16.0",
    "reflect-metadata": "^0.2.2",
    "tsyringe": "^4.10.0",
    "typeorm": "^0.3.24",
    "typescript": "^5.8.3",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.2",
    "@types/node": "^22.15.18",
    "@types/reflect-metadata": "^0.0.5",
